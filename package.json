{
  "name": "origin-envio",
  "version": "0.1.0",
  "scripts": {
    "clean": "find src/indexers -type d -name 'generated' -exec rm -rf {} + || true",
    "codegen": "pnpm run /^codegen:.*/ --parallel --aggregate-output",
    "start": "envio start -d src/indexers/collector --config config.yaml",
    "codegen:collector": "envio codegen -d src/indexers/collector && node scripts/fix-generated-package-names.mjs collector",
    "dev:collector": "envio dev -d src/indexers/collector",
    "start:collector": "envio start -d src/indexers/collector",
    "codegen:prices": "envio codegen -d src/indexers/prices && node scripts/fix-generated-package-names.mjs prices",
    "dev:prices": "envio dev -d src/indexers/prices",
    "start:prices": "envio start -d src/indexers/prices",
    "codegen:oToken": "envio codegen -d src/indexers/oToken && node scripts/fix-generated-package-names.mjs oToken",
    "dev:oToken": "envio dev -d src/indexers/oToken",
    "start:oToken": "envio start -d src/indexers/oToken"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/chai": "^4.3.20",
    "@types/mocha": "10.0.6",
    "@types/node": "20.8.8",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "chai": "4.3.10",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "mocha": "10.2.0",
    "prettier": "^3.7.4",
    "ts-mocha": "^10.1.0",
    "typescript": "5.2.2",
    "typescript-eslint": "^8.49.0"
  },
  "dependencies": {
    "dnum": "^2.17.0",
    "envio": "2.32.3",
    "viem": "^2.41.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}